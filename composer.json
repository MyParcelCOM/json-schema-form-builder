{
    "name": "myparcelcom/json-schema-form-builder",
    "description": "A library that provides an object-oriented API for constructing forms, which can then be converted into a JSON Schema.",
    "type": "library",
    "require": {
        "php": ">=8.4",
        "laravel/framework": ">=12.0",
        "ramsey/uuid": "^4.7",
        "opis/json-schema": "^2.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "phpunit/phpunit": ">=12.0",
        "roave/security-advisories": "dev-latest",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "MyParcelCom\\JsonSchema\\FormBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "MyParcel.com",
            "email": "info@myparcel.com"
        }
    ],
    "minimum-stability": "stable"
}
